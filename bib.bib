@article{mol2018,
  title = {International Child Abduction and the Best Interests of the Child: An Analysis of Judicial Reasoning in Two Jurisdictions},
  shorttitle = {International Child Abduction and the Best Interests of the Child},
  author = {Mol, Charlotte and Kruger, Thalia},
  date = {2018-09-02},
  journaltitle = {Journal of Private International Law},
  volume = {14},
  number = {3},
  pages = {421--454},
  publisher = {{Routledge}},
  issn = {1744-1048},
  doi = {10.1080/17441048.2018.1525074},
  url = {https://doi.org/10.1080/17441048.2018.1525074},
  urldate = {2020-05-07},
  abstract = {The Hague Child Abduction Convention aims to secure the speedy return of abducted children. Judges can use a limited number of grounds for refusal. They may not make an in-depth assessment of the merits of any custody issue. The Convention on the Rights of the Child provides that the best interests of the child shall be a primary consideration in all actions concerning children. This article analyses the use that judges make in their decisions on the concept of “the best interests of the child”. For this purpose it scrutinizes the case law on international child abduction of the Netherlands and England and Wales. By using software designed for qualitative research, the authors are able to make an objective and systematic analysis. This article confirms the hypothesis that the concept of the best interests of the child is often used without substance, and sometimes only to endorse conclusions that would have possibly been reached in any event.},
  keywords = {best interests of the child,Brussels IIa (Regulation 2201/2003),England and Wales,Hague Child Abduction Convention,international child abduction,return orders,second chance procedure,the Netherlands,wrongful removal,wrongful retention},
  annotation = {\_eprint: https://doi.org/10.1080/17441048.2018.1525074},
  file = {/Users/jgalmeida/Dropbox/Bibliografia/Biblioteca/Mol_Kruger_2018_International child abduction and the best interests of the child.pdf;/Users/jgalmeida/Zotero/storage/9NYH22RI/17441048.2018.html}
}

@article{momoh2019,
