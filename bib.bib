@article{fadlallah2001,
  title = {Colloque Arbitrage et fiscalité: Arbitrage international et litiges fiscaux},
  author = {Fadlallah, Ibrahim},
  date = {2001},
  journaltitle = {Revue de l'Arbitrage},
  number = {2},
  pages = {299--310},
  abstract = {In so far as taxation is a prerogative of the State, tax disputes appear mainly to fall outside the scope of arbitrators'jurisdiction. Nevertheless, tax disputes can arise in international arbitration, in circumstances where the solution to a dispute has major tax consequences, or where the subject matter of the dispute itself is taxation. An arbitrator can and must apply the relevant tax rules when they will determine the solution to a civil dispute, for example the validity of a document. State Courts can exercise control over this aspect, within the limits which they allow themselves to review compliance with public policy rules. The tax consequences of a solution under civil law may be numerous, but there will occasionally be cases where the principal subject matter of the dispute will be the tax consequences which the parties wish to draw therefrom. This will notably be the case where taxation will be dependent on an award, and such award is to be construed in order to enable the tax authorities to determine the taxable amount. The tax authorities would in such circumstances only be bound by the award to the same extent as they would be bound by any decision of the Civil Courts with the same subject matter. There may, however, also be circumstances where the parties disagree as to the levy of a tax or duty. The most common occurrence is that where a contract apportions such charge. Arbitrators must thereafter interpret the contract. The principal problem is to determine the relationship between the arbitration and a parallel taxation process. The question of stay of proceedings would thus arise, and the author proposes that arbitrators should not be bound to stay their proceedings, but should rule on the issue. Even more rarely, a State may seek to use the opportunity of proceedings relating to a contract in order to claim tax which would be payable as a result of such contract. This raises the question of the arbitrability of the dispute by reason of the subject matter. The author does not consider that such dispute would not be arbitrable by nature, in circumstances where the State accepts arbitration. However the agreement of the other party would be necessary in order to found the jurisdiction of the arbitrators. Arbitration clauses generally included in contracts do not cover this sort of dispute.},
  langid = {french},
  file = {/Users/jgalmeida/Dropbox/Bibliografia/Biblioteca/Fadlallah_2001_Colloque Arbitrage et fiscalité.pdf}
}

@report{fairfield2021,
  type = {SSRN Scholarly Paper},
  title = {Tokenized: {{The Law}} of {{Non-Fungible Tokens}} and {{Unique Digital Property}}},
  shorttitle = {Tokenized},
  author = {Fairfield, Joshua},
  date = {2021-04-06},
  number = {ID 3821102},
  institution = {{Social Science Research Network}},
  location = {{Rochester, NY}},
  url = {https://papers.ssrn.com/abstract=3821102},
  urldate = {2021-10-14},
  abstract = {Markets for unique digital property—digital equivalents of rare artworks, collectible trading cards, and other assets that gain value from scarcity—have exploded in the past several months. At root is the next iteration of blockchain technology, unique digital assets called non-fungible tokens. Unlike Bitcoin, where one coin is the same as another, NFTs are unique, each with different attributes. An NFT that represented ownership of Boardwalk would be quite different from one that represented Baltic Avenue. NFTs have grown from a few early breakout successes to a rapidly developing market for unique digital treasures. The attraction to buyers is that unlike digital assets like e-books or licensed movies, NFTs can be bought, sold, displayed, gifted, or even destroyed just like personal property. Yet law has not kept pace with demand for unique digital property. In particular, the rules designed for the 2000s internet focused on expanding intellectual property licenses and online contracts to the point that we are mere users, not owners, of digital assets. This article proposes a clear path for the evolution of the legal underpinnings of NFTs. It argues that NFTs are personal property, not contracts (despite the “smart contracts” popular nomenclature) or pure intellectual property licenses (despite the currently governing law of digital assets like e-books). Because transactions in NFTs are in the form of a sale, the law of sales of personal property should apply. And finally, the article notes that NFTs will serve as a powerful grounding example of digital personal property, a legal form of ownership that is both sorely needed and has not yet been clearly established online. That example will ground others, and permit law to again characterize those who buy scarce and valuable digital assets as true owners rather than mere users.},
  langid = {english},
  keywords = {bitcoin,crypto,cryptocurrency,digital,law,NFT,property,token,virtual},
  file = {/Users/jgalmeida/Dropbox/Bibliografia/Biblioteca/Fairfield_2021_Tokenized.pdf;/Users/jgalmeida/Zotero/storage/6FKEJYI8/papers.html}
}

