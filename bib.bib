@book{almeida2017,
  title = {O Divórcio em Direito Internacional Privado},
  author = {de Almeida, João Gomes},
  date = {2017},
  publisher = {{Almedina}},
  location = {{Coimbra}},
  abstract = {A deslocação transfronteiriça de pessoas é uma realidade cada vez mais generalizada, graças sobretudo ao progresso tecnológico. Dentro da União Europeia este fenómeno é igualmente potenciado pelas liberdades de circulação de pessoas, mercadorias, serviços e capitais. Hoje em dia, já não é invulgar encontrar casais de nacionalidades diferentes, casais com uma ou mais nacionalidades comuns, que residem habitualmente num outro Estado, e até casais (com ou sem nacionalidade comum) que residem habitualmente em diferentes Estados. E o futuro parece reservar apenas um ainda maior incremento destas situações. Em suma, as situações familiares transnacionais, isto é, as situações familiares que se encontram ligadas a mais de um Estado soberano, são cada vez mais comuns. De entre as situações familiares transnacionais, a presente dissertação foca-se no divórcio transnacional. O divórcio é a dissolução do casamento e, como tal, afeta de modo muito significativo a vida dos cônjuges, uma vez que extingue o próprio vínculo que os define como tal, fazendo cessar a relação familiar entre eles criada pelo casamento. O divórcio transnacional suscita questões específicas: perante as autoridades de que Estado soberano deve o divórcio ser requerido? Qual é o Direito material regulador do divórcio transnacional? E, proferida uma decisão relativa a um divórcio transnacional num determinado Estado soberano, essa decisão produz ou pode produzir os seus efeitos noutro Estado soberano? Estas questões específicas são respondidas, respetivamente, pelo Direito da Competência Internacional, Direito de Conflitos e Direito de Reconhecimento do divórcio. Assinala-se, todavia, que estas questões, embora distintas, não são estanques. Existem relações entre o Direito da Competência Internacional, o Direito de Conflitos e o Direito de Reconhecimento em matéria de divórcio que justificam uma análise conjunta, por forma a que não se perdam de vista essas relações e não se consagrem soluções incoerentes. A presente dissertação efetua um estudo global destas questões, da perspetiva do ordenamento jurídico português.},
  isbn = {978-972-40-7094-0},
  langid = {portuguese}
}

