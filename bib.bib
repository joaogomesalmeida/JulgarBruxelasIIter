@article{landbrecht2019,
  title = {The Concepts of “Injustice” and “Public Policy” in {{Article}} 6(c) of the {{Hague Choice}} of {{Court Convention}}},
  author = {Landbrecht, Johannes},
  date = {2019},
  journaltitle = {Journal of Private International Law},
  volume = {15},
  number = {2},
  pages = {339--364},
  publisher = {{Routledge}},
  issn = {1744-1048},
  doi = {10.1080/17441048.2019.1650457},
  url = {https://doi.org/10.1080/17441048.2019.1650457},
  urldate = {2020-05-07},
  abstract = {Article 6(c) of the Hague Choice of Court Convention (“Convention”) authorises courts seised (albeit not chosen) to ignore exclusive choice of court agreements at the jurisdictional stage if the effect of the agreement would lead to a “manifest injustice” or would be “manifestly contrary” to the public policy “of the State of the court seised”. Many seem to assume that only the domestic law of the State of the court seised matters in this context. Yet, a more nuanced interpretation is suggested in this article in the interest of furthering the Convention’s uniform interpretation. Article 6(c) establishes autonomously at treaty level concepts of injustice and public policy. These concepts do not exhaustively regulate when a court may rely on Article 6(c). However, they do define the potential scope (outer limits) of reliance on injustice and public policy concerns that a court seised (albeit not chosen) may make on the basis of domestic law.},
  keywords = {concurrent proceedings,exclusive choice of court agreements,Hague Choice of Court Convention 2005,manifest injustice,parallel proceedings,public policy},
  annotation = {\_eprint: https://doi.org/10.1080/17441048.2019.1650457},
  file = {/Users/jgalmeida/Dropbox/Bibliografia/Biblioteca/Landbrecht_2019_The concepts of “injustice” and “public policy” in Article 6(c) of the Hague.pdf;/Users/jgalmeida/Zotero/storage/9UMQWMLA/17441048.2019.html}
