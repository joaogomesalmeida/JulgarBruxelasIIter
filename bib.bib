@article{juarez2017,
  title = {La orden europea de retención de cuentas: ¿adiós a la dificultad que plantea el cobro de la deuda transfronteriza en la UE? = The European account preservation order: goodbye to the difficulty of collecting the cross-border debt in the eu?},
  shorttitle = {La orden europea de retención de cuentas},
  author = {Juárez, Isabel Antón},
  date = {2017-03-06},
  journaltitle = {Cuadernos de derecho transnacional},
