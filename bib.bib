  title = {Recasting {{West Tankers}} in the Deep Water: How {{Gazprom}} and Recast {{Brussels I}} Reconcile {{Brussels I}} with International Arbitration},
  shorttitle = {Recasting {{West Tankers}} in the Deep Water},
  author = {Farah, Youseph and Hourani, Sara},
