@article{font2019,
  title = {Recognition and Enforcement of Declaratory Awards under {{English Law}} as an Effective Method to Protect the Arbitration Agreement = {{Reconocimiento}} y Ejecución Del Laudo Declarativo En El {{Derecho}} Inglés Como Método Efectivo Para Proteger El Convenio Arbitral},
  author = {Font, Javier Pérez},
  date = {2019-10-01},
  journaltitle = {CUADERNOS DE DERECHO TRANSNACIONAL},
