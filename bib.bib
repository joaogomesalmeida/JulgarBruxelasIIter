@article{marino2017,
  title = {Strengthening the {{European Civil Judicial Cooperation}}: The Patrimonial Effects of Family Relationships = {{Verso}} Il Rafforzamento Della Cooperazione Giudiziaria Civile Nell’{{Unione Europea}}: Gli Effetti Patrimoniali Delle Relazioni Familiari},
