@article{dickinson2016,
  title = {Back to the Future: The {{UK}}’s {{EU}} Exit and the Conflict of Laws},
  shorttitle = {Back to the Future},
  author = {Dickinson, Andrew},
  date = {2016-05-03},
  journaltitle = {Journal of Private International Law},
  volume = {12},
