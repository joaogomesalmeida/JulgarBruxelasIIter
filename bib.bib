@article{gonzalez2019,
  title = {Ley Nacional del Causante, autonomía de la voluntad conflictual y reenvío. Una obra en tres actos. Breves notas a la sentencia del Tribunal Supremo de 15 enero 2019 = National law of the deceased, party autonomy and renvoi. A play in three acts. Comments on the spanish supreme court judgement of 15th January 2019},
  author = {González, Javier Carrascosa},
