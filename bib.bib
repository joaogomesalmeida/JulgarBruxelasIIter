  issn = {17441048},
  doi = {10.1080/17441048.2017.1345901},
  url = {http://search.ebscohost.com/login.aspx?direct=true&db=a9h&AN=124895587&lang=pt-br&site=eds-live},
  urldate = {2018-12-07},
  abstract = {During the recasting process of the EU Insolvency Regulation, issues relating to the relationship between the Regulation and the outer world were not debated. Indeed, the new Regulation (EU) 2015/848 maintains its territorial scope of application by making the application of the Regulation subject to the location of the centre of main interests within the territory of a Member State. This article tries to highlight the drawbacks of such geographical limitation concerning different aspects of the Regulation: in particular, jurisdiction, groups of companies, recognition of insolvency proceedings, cooperation and communication among courts and insolvency practitioners. Considering various possibilities to establish a truly universal regime, the article concludes that, in the light of the objective of an efficient administration of insolvency proceedings, the preferred approach is to extend the scope of application of the Regulation unilaterally, thereby including insolvencies significantly linked with third States.},
  keywords = {BANKRUPTCY,BUSINESS failures,COMMERCIAL law,COMMERCIAL law -- Government policy,EUROPEAN Union countries,Insolvency Regulation,NEW Zealand,outer world,recast,third States,universalism},
  file = {/Users/jgalmeida/Dropbox/Bibliografia/Biblioteca/Nisi_2017_The recast of the Insolvency Regulation.pdf}
}

@article{nobre2021,
  title = {Processos de comunicação digital no sistema educativo português em tempos de pandemia},
  author = {Nobre, Ana and Mouraz, Ana and Goulão, Maria de Fátima and Henriques, Susana and Barros, Daniela and Moreira, José António},
  date = {2021-04-01},
  journaltitle = {Práxis Educacional},
  volume = {17},
