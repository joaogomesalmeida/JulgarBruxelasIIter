@report{requejoisidro2017,
  type = {SSRN Scholarly Paper},
  title = {Reflections on the {{Preambles}} to the {{EU Private International Law Regulations}}},
  author = {Requejo Isidro, Marta},
  date = {2017-12-11},
  number = {ID 3212115},
  institution = {{Social Science Research Network}},
  location = {{Rochester, NY}},
  url = {https://papers.ssrn.com/abstract=3212115},
  urldate = {2019-01-19},
  abstract = {For a long time, a common rule of good legislative technique has been to keep preliminary texts short so that readers focus on the normative provisions.  However, one of the most striking features of recent EU regulations on private international law (PIL) is how long their preambles are. Why have they become so lengthy? What does this tell us about their nature and function? It is commonly asserted that preambles only have an interpretative value, and not even a privileged one: this is a widespread view also shared by the CJEU. Linked to it, evidence shows that textbooks on legal instruments devote just a cursory glimpse to preambles. We respectfully disagree with such habits and opinions: in the light of the current EU PIL regulations the debate about the value of preambles cannot be deemed settled. It is here submitted that their demotion to being mere interpretative tools is an oversimplification, and deserves to be revisited. In our view, preambles nowadays comprise plainly interpretative parts together with parts drafted in a way that suggests a somewhat different role. In other words, the possibility of a variety of values and functions - whatever opinion it may deserve - should not be automatically discarded.},
  langid = {british},
  keywords = {EU Private International Law,Preambles},
  file = {/Users/jgalmeida/Dropbox/Bibliografia/Biblioteca/Requejo Isidro_2017_Reflections on the Preambles to the EU Private International Law Regulations.pdf;/Users/jgalmeida/Zotero/storage/FJTWPF2M/papers.html}
}

